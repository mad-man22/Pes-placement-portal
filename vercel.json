{
    "rewrites": [
        {
            "source": "/api/jdoodle/:match*",
            "destination": "https://api.jdoodle.com/v1/:match*"
        },
        {
            "source": "/api/leetcode/:match*",
            "destination": "https://alfa-leetcode-api.onrender.com/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}